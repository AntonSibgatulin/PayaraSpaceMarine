<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="my.compary.psixol">

    <class name="Chapter" table="chapter">
        <meta attribute="class-description">
            This class contains the user detail.
        </meta>


        <id name="id" type="java.lang.Long" column="id">

        </id>



        <property name="name" column="name" type="java.lang.String"/>
        <property name="parentLegion" column="parentLegion" type="java.lang.String"/>
        <property name="marinesCount" column="marinesCount" type="java.lang.Integer"/>
        <property name="world" column="world" type="java.lang.String"/>


    </class>
</hibernate-mapping>
